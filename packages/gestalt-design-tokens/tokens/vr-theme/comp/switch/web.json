{
  "comp": {
    "switch": {
      "height": {
        "handle": {
          "value": "18px",
          "type": "dimension"
        }
      },
      "width": {
        "handle": {
          "value": "18px",
          "type": "dimension"
        }
      },
      "color": {
        "hover": {
          "background": {
            "container": {
              "enabled": {
                "off": {
                  "value": "{sema.color.hover.background.default}",
                  "type": "color"
                },
                "on": {
                  "value": "{sema.color.hover.background.selected}",
                  "type": "color"
                }
              }
            }
          }
        },
        "pressed": {
          "background": {
            "container": {
              "enabled": {
                "on": {
                  "value": "{sema.color.pressed.background.selected}",
                  "type": "color"
                },
                "off": {
                  "value": "{sema.color.pressed.background.default}",
                  "type": "color"
                }
              }
            }
          }
        },
        "focus": {
          "enabled": {
            "off": {
              "inner": {
                "value": "{sema.color.border.focus.inner.default}",
                "type": "color"
              },
              "outer": {
                "value": "{sema.color.border.focus.outer.default}",
                "type": "color"
              }
            },
            "on": {
              "inner": {
                "value": "{sema.color.border.focus.inner.default}",
                "type": "color"
              },
              "outer": {
                "value": "{sema.color.border.focus.outer.default}",
                "type": "color"
              }
            }
          }
        }
      }
    }
  }
}